    "test": "echo \"Error: no test specified\" && exit 1",
    "install_dep": "concurrently \"cd server && npm i\" \"cd client && npm i\"",
    "server": "concurrently \"cd client && npm start\" \"cd server && nodemon server.js --exec babel-node\"",
    "build-client": "cd client && npm run build",
    "build-server": "cd server && npm run build",
    "build": "concurrently --kill-others-on-fail \"npm run build-client\" \"npm run build-server\"",
    "heroku-prebuild": "NPM_CONFIG_PRODUCTION=false npm install && npm run install_dep",
    "start": "cd server && npm start"
  },
